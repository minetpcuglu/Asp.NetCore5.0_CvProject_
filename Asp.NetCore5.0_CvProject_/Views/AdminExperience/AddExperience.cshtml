@model Experience
@{
    ViewData["Title"] = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Yetenek Ekleme Sayfası</h2>
<br />
<form method="post" asp-controller="AdminExperience" asp-action="AddExperience">

    <div class="form-group">
        <label asp-for="Title"> <b>Deneyim</b></label>
        <input asp-for="Title" class="form-control" />
        @*<span asp-validation-for="CName" class="text-danger"></span>*@
    </div>
  
    <div class="form-group">
        <label asp-for="Date"> <b>Tarih</b></label>
        <input asp-for="Date" class="form-control" />
        @*<span asp-validation-for="CName" class="text-danger"></span>*@
    </div>
    <button type="submit" class="btn btn-outline-primary"><b>Ekle</b></button>
</form>